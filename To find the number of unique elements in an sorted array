class Main {
    public static void main(String[] args) {
        int a[]={2,2,3,3,30,30,40,40,40,50};
        int j=1;
        int i=0;
        while(j<a.length){
               if(a[i]!=a[j]){
                   a[i+1]=a[j];
                   i++;
                   j++;
               }
               else if(a[i]==a[j]){
                   j++;
               }
        }
        System.out.println(i+1);
    }
}
