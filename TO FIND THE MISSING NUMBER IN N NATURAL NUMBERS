import java.util.*;
class Main {
    public static void main(String[] args) {
     Scanner sc=new Scanner(System.in);
      int n=sc.nextInt();
       int a[]=new int[n-1];
       int c=0;
       int o=0;
       int mn=0;
       for(int i=0;i<n-1;i++){
           a[i]=sc.nextInt();
       }
       for(int i=0;i<n-1;i++){
           c+=a[i];
       }
       o=n*(n+1)/2;
       mn=o-c;
       System.out.println("MISSING");
       System.out.println(mn);
       
    }
}
