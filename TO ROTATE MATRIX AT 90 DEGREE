import java.util.*;
class Main {
    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int m=n;
    int c=0;
    int cv=0;
    int [][]a=new int[n][n];
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            a[i][j]=sc.nextInt();
        }
    }for(int j=0;j<m;j++){
        for(int i=n-1;i>=0;i--){
          
         System.out.print(a[i][j]+" ");
        }        System.out.println();
        }
        
    }
    }
        
        
       
