import java.util.*;

class Main {
    public static void main(String[] args) {
       int n=1;
       int m=4;
      
       int sum=0;
      
        while(n<=m){
            
       for(int i=1;i<=n;i++){
           if(n%i==0){
               sum+=i;
           }
       }n++;
        }
        
        System.out.println(sum);
      
    }
}
